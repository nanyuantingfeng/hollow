  "scripts": {
    "clean": "rm -rf lib && rm -rf build",
    "build": "npm run clean && babel src --out-dir lib",
    "build:copy": "mkdir -p build && rsync -av --exclude='node_modules' --exclude='demo' --exclude='build' ./ ./build",
    "publish2npm": "npm run build && npm run build:copy && npm publish ./build",
    "test": "jest --coverage"
  },
