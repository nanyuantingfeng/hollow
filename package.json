{
  "name": "hollow-cli",
  "version": "2.0.0-beta.3",
  "description": "Hollow CLI",
  "license": "MIT",
  "main": "./lib/index.js",
  "module": "./esm/index.js",
  "types": "./esm/index.d.ts",
  "scripts": {
    "build": "npm run tsc && npm run optimize && npm run copy",
    "optimize": "npm run babel:lib & npm run babel:esm",
    "copy": "cp package.json README.md .npmignore dist",
    "babel:lib": "BABEL_ENV=lib babel dist/lib --out-dir dist/lib",
    "babel:esm": "BABEL_ENV=esm babel dist/esm --out-dir dist/esm",
    "publish2npm": "npm run build && npm publish dist",
    "lint:es": "eslint --ext .jsx,.js src",
    "lint:ts": "tslint -p tsconfig.json",
    "test:watch": "jest -o --watch",
    "test": "jest --coverage",
    "tsc": "rm -rf dist && tsc -p tsconfig.esm.json && cp -R dist/esm dist/lib"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "devDependencies": {
    "@types/autoprefixer": "^9.4.0",
    "@types/case-sensitive-paths-webpack-plugin": "^2.1.2",
    "@types/copy-webpack-plugin": "^4.4.3",
    "@types/html-webpack-plugin": "^3.2.0",
    "@types/jest": "^24.0.11",
    "@types/koa-compose": "^3.2.3",
    "@types/lodash-webpack-plugin": "^0.11.3",
    "@types/mini-css-extract-plugin": "^0.2.0",
    "@types/node": "^11.12.2",
    "@types/node-notifier": "^5.4.0",
    "@types/optimize-css-assets-webpack-plugin": "^1.3.4",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "@types/terser-webpack-plugin": "^1.2.1",
    "@types/webpack": "^4.4.26",
    "@types/webpack-bundle-analyzer": "^2.13.1",
    "@types/webpack-dev-server": "^3.1.4",
    "@types/webpack-manifest-plugin": "^2.0.0",
    "@types/workbox-webpack-plugin": "^4.1.0",
    "antd": "^3.15.2",
    "babel-jest": "^24.5.0",
    "copy-dir": "^0.4.0",
    "coveralls": "^3.0.3",
    "glob": "^7.1.3",
    "jest": "^24.5.0",
    "lodash": "^4.17.11",
    "prettier": "^1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "shelljs": "^0.8.3",
    "ts-jest": "^24.0.1",
    "tslint": "^5.14.0",
    "tslint-config-prettier": "^1.18",
    "typescript": "^3"
  },
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.4.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-proposal-decorators": "^7.4.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
