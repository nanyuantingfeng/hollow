  "bin": {
    "hollow": "./bin/index"
  },
  "scripts": {
    "clean": "rm -rf lib && rm -rf build",
    "build:copy": "mkdir -p build && cp -R assets bin lib package.json README.md build",
    "build": "npm run clean && node_modules/.bin/babel src --out-dir lib && npm run build:copy",
    "publish2npm": "npm run build && npm publish ./build",
    "test": "jest",
    "test:cover": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
