  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanyuantingfeng/hollow-cli.git"
  },
  "keywords": [
    "hollow"
  ],
  "bugs": {
    "url": "https://github.com/nanyuantingfeng/hollow-cli/issues"
  },
  "homepage": "https://github.com/nanyuantingfeng/hollow-cli#readme",
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "json5",
      "node"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.{ts,tsx,js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "/dist/"
    ]
  }
}
