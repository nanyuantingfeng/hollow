  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json",
      "json5",
      "node",
      "gql",
      "graphql"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.{ts,tsx,js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "/dist/"
    ]
