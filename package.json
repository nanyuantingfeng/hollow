{
  "name": "hollow-cli",
  "version": "1.4.32",
  "description": "Hollow CLI",
  "main": "lib/index.js",
  "author": "nanyuantingfeng",
  "license": "MIT",
  "email": "nanyuantingfeng@aliyun.com",
  "bin": {
    "hollow": "./bin/index"
  },
  "scripts": {
    "build": "npm run clean && npm run optimize && npm run cp:files",
    "optimize": "npm run babel:lib",
    "babel:lib": "babel src --out-dir dist/lib --plugins=@babel/plugin-transform-modules-commonjs",
    "cp:files": "cp -R assets bin package.json tsconfig.json README.md dist",
    "clean": "rm -rf dist && mkdir dist",
    "publish2npm": "npm run build && npm publish ./dist",
    "test": "jest",
    "test:cover": "jest --coverage",
    "coveralls": "cat ./coverage/lcov.info | coveralls"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "copy-dir": "^0.4.0",
    "coveralls": "^3.0.2",
    "glob": "^7.1.3",
    "jest": "^23.6.0",
    "lodash": "^4.17.11",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "shelljs": "^0.8.3",
    "ts-jest": "^23.10.5",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@babel/cli": "^7.2.0",
    "@babel/core": "^7.2.0",
    "@babel/plugin-external-helpers": "^7.2.0",
    "@babel/plugin-proposal-async-generator-functions": "^7.2.0",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@babel/plugin-proposal-decorators": "^7.2.0",
    "@babel/plugin-proposal-function-bind": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-export-default-from": "^7.2.0",
    "@babel/plugin-syntax-export-namespace-from": "^7.2.0",
    "@babel/plugin-syntax-function-bind": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-object-assign": "^7.2.0",
    "@babel/plugin-transform-regenerator": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@svgr/webpack": "^4.1.0",
    "autoprefixer": "^9.4.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-lodash": "^3.3.4",
    "case-sensitive-paths-webpack-plugin": "^2.1.2",
    "chalk": "^2.4.1",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^1",
    "file-loader": "^2.0.0",
    "happypack": "^5.0.0",
    "html-webpack-plugin": "^3.2.0",
    "json5-loader": "^1.0.1",
    "koa-compose": "^4.1.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.5.0",
    "mustache-loader": "^1.4.1",
    "node-notifier": "^5.3.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss": "^7.0.6",
    "postcss-loader": "^3.0.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.1.0",
    "ts-loader": "^5.3.1",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "typescript": "^3",
    "webpack": "^4.27.1",
    "webpack-bundle-analyzer": "^3.0.3",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.10",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-pwa-manifest": "^4.0.0",
    "workbox-webpack-plugin": "^3.6.3",
    "worker-loader": "^2.0.0",
    "yargs": "^12.0.5"
  },
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nanyuantingfeng/hollow-cli.git"
  },
  "keywords": [
    "hollow"
  ],
  "bugs": {
    "url": "https://github.com/nanyuantingfeng/hollow-cli/issues"
  },
  "homepage": "https://github.com/nanyuantingfeng/hollow-cli#readme",
  "jest": {
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "<rootDir>/test/**/*.test.{ts,tsx,js,jsx}"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/fixtures/"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__tests__/",
      "/dist/"
    ]
  }
}
